<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{7f57cf46-4467-4c2d-adfa-0cba7c507e54}</em:id>
    <em:version>0.4.1</em:version>

    <!-- Target Application this extension can install into,
        with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>.9</em:minVersion>
        <em:maxVersion>.10+</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>Mozilla Archive Format</em:name>
    <em:description>Save complete web pages in a single file.</em:description>
    <em:creator>Christopher Ottley</em:creator>
    <em:contributor>Gioxx: eXtenZilla.it</em:contributor>
    <em:contributor>Xavier Robin</em:contributor>
    <em:homepageURL>http://maf.mozdev.org/</em:homepageURL>


    <!-- Front End Integration Hooks (used by Extension Manager)-->
    <em:optionsURL>chrome://maf/content/mafPreferencesDLG.xul</em:optionsURL>
    <!-- <em:aboutURL>chrome://maf/content/mafAboutDLG.xul</em:aboutURL> -->
    <!-- <em:iconURL>chrome://maf/skin/maf.png</em:iconURL> -->

    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:maf.jar">
        <em:package>content/</em:package>
        <em:locale>locale/en-US/</em:locale>
        <em:locale>locale/it-IT/</em:locale>
        <em:locale>locale/fr-FR/</em:locale>
        <!-- <em:skin>skin/classic/maf/</em:skin> -->
      </Description>
    </em:file>
  </Description>
</RDF>
